{
  "name": "hspice-netlist",
  "displayName": "HSPICE Language Support",
  "description": "Syntax highlighting and snippets for HSPICE",
  "version": "1.0.0",
  "publisher": "MuhammadShofuwanAnwar",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shofuuu/hspice-netlist.git"
  },
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "hspice",
      "aliases": ["HSPICE", "hspice"],
      "extensions": [".sp",".cir",".spice",".hspice"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "hspice",
      "scopeName": "source.sp",
      "path": "./syntaxes/hspice.tmLanguage.json"
    }],
    "snippets": [
        {
            "language": "hspice",
            "path": "./snippets/hspice.code-snippets"
        }
    ]
  }
}
